{
  "name": "noclaw",
  "version": "1.0.0",
  "description": "Interactive CLI tool for detecting and removing Claw agents (OpenClaw, IronClaw, NanoClaw, PicoClaw, and legacy variants)",
  "module": "src/index.ts",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "noclaw": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "uninstaller",
    "systemd",
    "docker",
    "noclaw",
    "openclaw",
    "ironclaw"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build --compile --target=bun-linux-x64 src/index.ts --outfile noclaw",
    "build:npm": "tsup",
    "prepublishOnly": "bun run build:npm",
    "start": "./noclaw"
  },
  "dependencies": {
    "@clack/prompts": "^0.9.1",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.5.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}